VLIBRAS_VIDEO_TOKEN=
# polling
VLIBRAS_POLL_INTERVAL_S=1.0
VLIBRAS_POLL_TIMEOUT_S=180

# diretório de saída
OUT_DIR=videos

# log
LOG_LEVEL=INFO