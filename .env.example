# tradução (ok como está)
VLIBRAS_TRANSLATE_BASE_URL=https://traducao2.vlibras.gov.br

# vídeo (local)
VLIBRAS_VIDEO_BASE_URL=http://127.0.0.1:8080

# JWT gerado via: docker exec vlibras_video_api node -e "..."
